
def compare(x,y):    # THIS FUNCTION WAS MADE TO COMPARE NUMBERS
	if x < y:
	    print x, "is less than", y
	elif x > y:
	    print x, "is greater than", y
	else:
	    print x, "and", y, "are equal"

cat = input ("write x ")
bet = input ("write y ")

compare(cat,bet)	# THIS A GENERAL WAY
compare(3,4)		# THOSE ARE THE COMMON WAY
compare(4,3)
compare(5,5)
compare(3.1,3.2)
