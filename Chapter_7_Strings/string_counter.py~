def print_strings(n):
    """
      >>> print_strings("isjfnisuhfia",3,7)
      fnisu
      >>> print_strings("idgidhfgiudfg",5,9)
      hfgiu
      >>> print_strings("jkdngidhiuvgdsiuf",1,10)
      kdngidhiuv
    """
    return string_counter(n,i,j)

